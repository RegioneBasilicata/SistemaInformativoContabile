	<parameter name="UTENTE" class="java.lang.String"/>
	<parameter name="ANNO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
decode(b.tipo,'A','ATTIVO','PASSIVO')  Gruppo_P,
b.conto_stampa campo_P1,
decode(b.s_voce,'',to_char(b.voce),b.s_voce) campo_P2,
b.dettaglio campo_P3,
b.tipo tipoP,
b.conto_vis codiceP,
b.descrizione descrizione_P,
b.classe classeP,
b.s_voce s_voceP,
b.dettaglio dettaglioP,
b.s_classe s_classeP,
sum(nvl(c.IMPORTO,0)) importo_P,
sum(nvl(c.IMPORTO1,0)) importo_Q,
$P{ANNO} anno,
$P{ANNO} -1 anno_prec,
decode(b.attributo_stampa,'G',1,'S',2,'C',3,'T',4,0) fogliaP,
b.voce voceP,
b.ordinamento_stampa ordP
,b.attributo_stampa attributo_stampaP
FROM
nb_piacee_importi c 
,nb_piacee b
WHERE
c.anno(+) = $P{ANNO}
and c.utente(+)  = $P{UTENTE} 
and b.tipo = c.tipo_piacee(+)
and b.conto_vis = c.codice_piacee(+)
and b.tipo in ('P','CO')
group by
b.tipo,
b.conto_stampa,
$P{ANNO},
$P{ANNO} -1,
b.tipo,
b.conto_vis,
b.descrizione,
b.classe,
b.s_classe,
b.s_voce,
b.dettaglio,
decode(b.attributo_stampa,'G',1,'S',2,'C',3,'T',4,0),
b.voce,
b.ordinamento_stampa,
b.attributo_stampa
order by b.ordinamento_stampa]]>
	</queryString>
